{
    "name": "brizhanev/laravel-ulogin",
    "description": "The package adds ability to authorize via social networks with Ulogin in Laravel 5+ projects.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Brizhanev",
            "email": "mr.brizhanev@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravelcollective/html": "^5.4.0"
    },
    "autoload": {
        "classmap":[
            "src/controllers",
            "src/models"
        ],
        "psr-4": {
            "Brizhanev\\LaravelUlogin\\": "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Brizhanev\\LaravelUlogin\\LaravelUloginServiceProvider"
            ]
        }
    }
}
